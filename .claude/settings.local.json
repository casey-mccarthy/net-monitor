{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(kill:*)",
      "Bash(pkill:*)",
      "Bash(cargo run:*)",
      "Bash(cargo check:*)",
      "Bash(cargo --version)",
      "Bash(cargo:*)",
      "Bash(cargo fmt)",
      "Bash(cargo fmt -- --check)",
      "Bash(RUST_BACKTRACE=1 cargo run)",
      "Bash(RUSTFLAGS=\"-A dead_code\" cargo test --test integration_tests)",
      "Bash(RUSTFLAGS=\"-A dead_code\" cargo clippy:*)",
      "Bash(RUSTFLAGS=\"-A dead_code\" cargo test:*)",
      "Bash(RUSTFLAGS=\"-A dead_code\" cargo build:*)",
      "Bash(set RUSTFLAGS=-A dead_code)",
      "Bash(rustc:*)",
      "Bash(rustup target:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git stash:*)",
      "Bash(git tag:*)",
      "Bash(git merge-base:*)",
      "Bash(git log:*)",
      "Bash(git checkout:*)",
      "Bash(git fetch:*)",
      "Bash(git rebase:*)",
      "Bash(git push:*)",
      "Bash(git status)",
      "Bash(gh pr list:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr edit:*)",
      "Bash(gh pr:*)",
      "Bash(gh run list:*)",
      "Bash(gh run view:*)",
      "Bash(gh api:*)",
      "Bash(gh issue list:*)",
      "Bash(gh issue view:*)",
      "Bash(find:*)",
      "Bash(cat:*)",
      "Bash(wc -l:*)",
      "Bash(ls:*)",
      "Bash(cd:*)",
      "Bash(pwd)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(RUSTFLAGS*)",
      "Bash(RUSTFLAGS=* && *)",
      "Bash(cargo audit:*)",
      "Bash(cargo clippy:*)",
      "Bash(cargo doc:*)",
      "Bash(cargo install:*)",
      "Bash(RUSTFLAGS='-A dead_code' cargo build --release)",
    ],
    "deny": [],
    "ask": []
  }
}
